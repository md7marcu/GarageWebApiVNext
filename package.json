{
  "name": "gatewebapi",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "auto": "./node_modules/nodemon/bin/nodemon.js",
    "test": "cross-env NODE_ENV=windows mocha -exit --timeout '60000' -r ts-node/register test/**/*.spec.ts",
    "test-dev": "cross-env NODE_ENV=windows nodemon -L --watch . --ext ts --exec \"mocha --timeout '60000' -r ts-node/register test/**/*.spec.ts\"",
    "config-test-dev": "cp -R lib/views dist && cp -R public dist/public && DEBUG=bajs NODE_ENV=test nodemon --watch . --ext ts --exec \"env-cmd -f ./config/.env  mocha -r ts-node/register test/**/*.spec.ts\"",
    "build": "tsc && cp ./config/cert.pem config/serverCert.pem",
    "dev": "cross-env DEPLOYMENT=staging ts-node ./lib/server.ts",
    "start": "cross-env DEPLOYMENT=staging nodemon ./dist/server.js",
    "prod": "cross-env DEPLOYMENT=production npm run build && npm run start",
    "postinstall": "node-config-ts",
    "windows": "cross-env NODE_ENV=windows ts-node ./lib/server.ts",
    "watch": "tsc -w -p ."
  },
  "author": "",
  "license": "ISC",
  "optionalDependencies": {
    "node-wiring-pi": "0.0.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "debug": "^4.1.1",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "guid-typescript": "^1.0.9",
    "http": "0.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.22.2",
    "mongoose": "^5.13.5",
    "mqtt": "^4.2.8",
    "node-config-ts": "^3.1.0",
    "node-forge": "^0.10.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "path": "^0.12.7",
    "pg": "^7.5.0",
    "socket.io": "^2.4.1",
    "system-sleep": "^1.3.7",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-spies": "^1.0.3",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.16.0",
    "@types/mocha": "^8.2.0",
    "@types/node": "^10.17.51",
    "@types/passport": "^1.0.0",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/socket.io": "^1.4.36",
    "chai": "^4.2.0",
    "chai-spy": "0.0.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "ts-sinon": "^2.0.1",
    "tslint": "^6.1.3"
  }
}
